{
  "name": "node-ejs",
  "main": "server.js",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "ejs": "^1.0.0",
    "electron": "^1.8.7",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mysql": "^2.15.0",
    "mysql-migrations": "^1.0.6",
    "node-querybuilder": "^1.1.1",
    "require-all": "^2.2.0",
    "squel": "^5.12.1"
  },
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "migration": "node database/migration",
    "artisan": "node artisan"
  },
  "devDependencies": {
    "nodemon": "^1.17.4"
  }
}
